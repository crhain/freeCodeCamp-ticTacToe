{
  "name": "fcc-tictactoe",
  "version": "1.0.0",
  "description": "A simple game of tic tac toe",
  "homepage": "http://crhwebdev.github.io/fcc-tictactoe",
  "main": "index.js",
  "scripts": {
    "combine": "rollup ./src/app/app.js -o ./public/app/app.js -f iife --name \"game\"",
    "bundle": "rollup -c",
    "start": "parcel index.html --open",
    "lint": "eslint js",
    "build": "parcel build index.html --public-url ./",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "postdeploy": "rimraf dist",
    "test": "mocha"
  },
  "author": "Carl Hain",
  "license": "MIT",
  "dependencies": {
    "model.js": "0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.11",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.16.1",
    "eslint-config-google": "^0.7.1",
    "gh-pages": "^1.1.0",
    "mocha": "^3.2.0",
    "parcel-bundler": "^1.7.0",
    "rimraf": "^2.6.2"
  }
}
